{
  "expo": {
    "name": "First Cast",
    "slug": "first-cast",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003362"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.first.cast",
      "config": {
        "googleMapsApiKey": "AIzaSyDCG-KE-wuY-T3gNabidFduJJeBBpytWuY"
      },
      "LSMinimumSystemVersion": "13.0",
      "minimumDeploymentTarget": "13.0",
      "NSLocationWhenInUseUsageDescription": "We need your location to select the closest part in the map",
      "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to select the closest part in the map",
      "permissions": {
        "location": {
          "reason": "We need your location to select the closest part in the map"
        }
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB-MSaTA15IDjYXUGJPgI6qgVcuVAaEkX4"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#003362"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET"
      ],
      "package": "com.first.cast"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to use your location to get the right lake",
          "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to use your location to get the right lake",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to get the right lake."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e35353c4-bf96-4b37-8fa2-87abd86c40cf"
      }
    }
  }
}
